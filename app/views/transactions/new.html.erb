                  <a class="item" data-tab="member">MEMBER</a>
               </div>
               <div class="ui tab" data-tab="transaction">
                  <div class="ui form">
                     <div class="five fields">
                        <div class="field">
                           <label>KODE BARANG</label>
                           <input type="text" placeholder="123456789" id="itemId"/>
                        </div>
                        <div class="field">
                           <label>JUMLAH</label>
